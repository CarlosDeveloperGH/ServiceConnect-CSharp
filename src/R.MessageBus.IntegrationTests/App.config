<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="BusSettings" type="BusSettings.BusSettings, R.MessageBus.Settings" />
  </configSections>

  <BusSettings xmlns="BusSettings">
    <TransportSettings>
      
      <RabbitMq EndPoint="Default" Host="localhost">
        <Exchange Name="TestExchange1" Type="topic" Durable="false" AutoDelete="true" />
        <Retries MaxRetries="1" RetryDelay="1000" />
        <Queue Name="TestQueue" RoutingKey="TestQueueRoutingKey" AutoDelete="true" Exclusive="true" Durable="true">
          <arguments>
            <Argument Name="Arg" value="Val"/>
          </arguments>
        </Queue>
      </RabbitMq>
    </TransportSettings>

    <PersistanceSettings>
      <MongoDb Database="TestDatabase" ConnectionString="mongodb://localhost/" />
    </PersistanceSettings>
  </BusSettings>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>

</configuration>